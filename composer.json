{
  "name": "kanopi/kanopi_event_calendar",
  "type": "drupal-custom-module",
  "description": "Drupal 8 module that installs an Events Content Type and Calendar views.",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://github.com/kanopi/kanopi_event_calendar",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://github.com/kanopi/kanopi_event_calendar/issues",
    "source": "https://github.com/kanopi/kanopi_event_calendar"
  },
  "require": {
    "drupal/address": "^1.0.0",
    "drupal/date_recur": "dev-1.x#8bd0ca37",
    "drupal/calendar": "dev-1.x#73e2979f",
    "drupal/media_entity_browser": "^2.0",
    "drupal/metatag": "^1.5",
    "drupal/pathauto": "^1.0",
    "drupal/schema_metatag": "^1.0",
    "commerceguys/addressing": "^1.0.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
    },
  "scripts": {
    "post-update-cmd": [
      "@clean-vcs"
    ],
    "post-install-cmd": [
      "@clean-vcs"
    ],
  "clean-vcs": "find vendor -type d | grep .git | xargs rm -rf"
  }
}
